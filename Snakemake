
# for now read all pdfs in the demo directory and write all outputs to the output directory

# Rule that accepts a pdf and runs it through OCR

rule ocr:
    input:
        "input.pdf"
    output:
        "output.pdf"
    shell:
        "ocrmypdf --language-detection --redo-ocr --deskew --rotate-pages --optimize 2 {input} {output}"
